{
  "name": "dea",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts",
    "dev": "bun --watch src/index.ts",
    "deploy:commands": "bun src/scripts/deploy-commands.ts",
    "check:lint-format": "bun src/scripts/check-lint-format.ts",
    "format": "bun src/scripts/format.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--experimental-vm-modules' bun jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' bun jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' bun jest --coverage",
    "clean": "rm -rf node_modules bun.lockb && bun install"
  },
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "discord.js": "^14.23.2",
    "fluent-ffmpeg": "^2.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
